{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "master"
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "lint": {
      "cache": true
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": []
  },
  "generators": {
    "@nx/js:library": {
      "buildable": true,
      "publishable": true,
      "importPath": "@devtoolbox/{projectName}"
    },
    "@nx/nest:library": {
      "buildable": true,
      "publishable": true
    }
  },
  "nxCloudAccessToken": "MTU2NWI3YjQtZTUxNi00MjFjLTk5NmQtMTg5YjE5MmY2NzRmfHJlYWQtd3JpdGU="
}
